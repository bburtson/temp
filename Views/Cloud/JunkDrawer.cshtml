@model IEnumerable<string>
@{
    ViewBag.Title = "Junk Drawer";
}
<div class="container-fluid col-xs-12 col-md-offset-2 col-md-8">
    <table class="table table-striped table-hover large">
        <thead>
            <tr>
                <th>Url</th>
            </tr>
        </thead>

        @foreach (var img in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink($"/cloud/junkdrawer/{img}", "JunkDrawer", new { fileName = $"{img}" })
                </td>
            </tr>
        }

    </table>
</div>
